<h1>Users</h1>
<table>
  <tr>
    <th>Username</td>
    <th>Emails</td>
    <th>Authentication Methods</td>
  </tr>
  <% for user in @users -%>
  <tr>
    <td>
      <%= link_to user.username, user_path( user.id ) %>
    </td>
    <td>
      <%= user.emails.join(', ') %>
    </td>
    <td>
      <%= user.auth.keys.join(', ') %>
    </td>
  </tr>
  <% end -%>
</table>